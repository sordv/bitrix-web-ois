{
    "require": {
        "nette/php-generator": "^3.2",
        "symfony/console": "^4.1",
        "doctrine/event-manager": "^1.1"
    },
    "config": {
        "platform": {
            "php": "8.1"
        }
    },
    "replace": {
        "psr/log":  "*",
        "psr/container":  "*",
        "psr/http-client":  "*",
        "psr/http-message":  "*"
    },
    "autoload": {
        "psr-4": {
            "Legacy\\": "classes/",
            "Legacy\\API\\": "api/classes/",
            "Console\\": "console/"
        }
    }
}
